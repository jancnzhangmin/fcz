                <DataGridTextColumn Binding="{Binding Path=built_up_area}" Header="建筑面积" />
                <DataGridTextColumn Binding="{Binding Path=right_value}" Header="权利价值" />
                <DataGridTextColumn Binding="{Binding Path=duration_right}" Header="权利存续期限" />
