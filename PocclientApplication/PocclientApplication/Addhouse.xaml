             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="950" d:DesignWidth="1000" xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml">
    <Grid x:Name="addhousegrid" Loaded="addhousegrid_Loaded">
        
